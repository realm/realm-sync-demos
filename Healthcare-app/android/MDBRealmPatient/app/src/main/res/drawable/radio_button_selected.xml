<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="20dp"
        android:height="20dp">
<shape android:shape="oval">
    <solid android:color="@color/transparent" />
    <stroke
        android:width="1.5dp"
        android:color="@color/app_new_color" />
    <size
        android:width="20dp"
        android:height="20dp" />
</shape>
    </item>
    <item android:top="4dp"
        android:start="4dp"
        android:width="12dp"
        android:height="12dp">
        <shape android:shape="oval">
            <solid android:color="@color/app_new_color" />

            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>
</layer-list>